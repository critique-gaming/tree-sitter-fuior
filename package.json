{
  "name": "tree-sitter-fuior",
  "version": "0.7.0",
  "description": "Grammar for Fuior (narrative design DSL)",
  "main": "bindings/node",
  "scripts": {
    "generate": "tree-sitter generate",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --strip --all -x node@5.0.0 -x node@6.0.0 -x electron@0.36.0 -x electron@1.1.0 -x electron@1.3.0 -x electron@1.4.0 -x node@17.0.0 && prebuildify --strip -t node@17.0.1",
    "test": "tree-sitter test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/critique-gaming/tree-sitter-fuior.git"
  },
  "author": "Marius Petcu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/critique-gaming/tree-sitter-fuior/issues"
  },
  "homepage": "https://github.com/critique-gaming/tree-sitter-fuior#readme",
  "dependencies": {
    "nan": "^2.15.0",
    "node-gyp-build": "^4.3.0"
  },
  "devDependencies": {
    "prebuildify": "github:dapetcu21/prebuildify",
    "tree-sitter-cli": "^0.19.5"
  },
  "tree-sitter": {
    "scope": "source.fuior",
    "file_types": [
      "fui"
    ],
    "highlights": [
      "queries/highlights.scm"
    ]
  }
}
