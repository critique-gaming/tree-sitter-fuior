{
  "name": "tree-sitter-fuior",
  "version": "0.2.0",
  "description": "Grammar for Fuior (narrative design DSL)",
  "main": "index.js",
  "scripts": {
    "build": "tree-sitter generate && node-gyp build --debug",
    "install": "prebuild-install || node-gyp rebuild",
    "prebuild": "prebuild -r electron -t 1.8.0 -t 2.0.0 -t 3.0.0 -t 4.0.0 --strip && prebuild -t 8.16.0 -t 10.16.0 --strip",
    "prebuild:upload": "prebuild --upload-all",
    "test": "tree-sitter test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dapetcu21/tree-sitter-fuior.git"
  },
  "author": "Marius Petcu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dapetcu21/tree-sitter-fuior/issues"
  },
  "homepage": "https://github.com/dapetcu21/tree-sitter-fuior#readme",
  "dependencies": {
    "nan": "^2.14.0",
    "prebuild-install": "^5.3.0"
  },
  "devDependencies": {
    "prebuild": "^9.0.1",
    "tree-sitter-cli": "^0.15.7"
  }
}
